{
  "version": 2,
  "builds": [
    {
      "src": "SearchMethods/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "SearchMethods/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "SearchMethods.settings",
    "VERCEL": "1"
  }
}
